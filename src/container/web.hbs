<main>
    {{#with web}}
    <h2>Languages:</h2>

    <ol>
        {{#with languages.[0]}}
        <li>
            <b>Name: {{name}}</b>
            <p>Version: {{version}}</p>
            <p>Description: {{description}}</p>


            <ul>
                <h4>Elements:</h4>
                {{#each elements}}
                <li>
                    <p>name: {{name}}</p>
                    <p>description: {{description}}</p>

                    <ul>
                        <h4>Attributes:</h4>
                        {{#each attributes}}
                        <li>
                            <p>name: {{name}}</p>
                            <p>description: {{description}}</p>
                        </li>
                        {{/each}}
                    </ul>
                </li>
                {{/each}}
            </ul>
        </li>
        <hr>
        <br>
        {{/with}}


        {{#with languages.[1]}}
        <li>
            <b>Name: {{name}}</b>
            <p>Version: {{version}}</p>
            <p>Description: {{description}}</p>


            <ul>
                <h4>Properties:</h4>
                {{#each properties}}
                <li>
                    <p>name: {{name}}</p>
                    <p>description: {{description}}</p>

                    <ul>
                        <h4>Values:</h4>
                        {{#each values}}
                        <li>
                            <p>value: {{value}}</p>
                            <p>description: {{description}}</p>
                        </li>
                        {{/each}}
                    </ul>
                </li>
                {{/each}}
            </ul>
        </li>
        <hr>
        <br>
        {{/with}}

        {{#with languages.[2]}}
        <li>
            <b>Name: {{name}}</b>
            <p>Version: {{version}}</p>
            <p>Description: {{description}}</p>


            <ul>
                <h4>Functions:</h4>
                {{#each functions}}
                <li>
                    <p>name: {{name}}</p>
                    <p>description: {{description}}</p>

                    <ul>
                        <h4>Parameters:</h4>
                        {{#each parameters}}
                        <li>
                            <p>name: {{name}}</p>
                            <p>type: {{type}}</p>
                            <p>description: {{description}}</p>
                        </li>
                        {{/each}}
                    </ul>
                </li>
                {{/each}}
            </ul>
        </li>
        <hr>
        <br>
        {{/with}}

    </ol>
    {{/with}}
</main>