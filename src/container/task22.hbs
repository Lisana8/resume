                    <div class="card">
                        <div class="card-body">
                            <div>{{section.agreement.info.detail.text}}</div>
                            <br>
                            <button type="button" class="btn btn-success">
                                {{section.agreement.info.detail.button}}
                            </button>
                        </div>
                    </div>

                </div>
            </div>

        </div>
