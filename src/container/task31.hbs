<main>
    <div>

        <section class="row">
            {{#with navigation}}
            <a href="{{links.[0].href}}" class="btn btn-link col-3" target="_blank">{{links.[0].text}}</a>
            <a href="{{links.[1].href}}" class="btn btn-link col-3" target="_blank">{{links.[1].text}}</a>
            <a href="{{links.[2].href}}" class="btn btn-link col-3" target="_blank">{{links.[2].text}}</a>
            <a type="button" href="{{button.href}}" class="btn btn-primary col-3" target="_blank">
                {{button.text}}
            </a>
            {{/with}}
        </section>

        <hr>

        <section>
            {{#with header}}
            <h1>{{title}}</h1>
            <p>{{description}}</p>
            <a href="{{button.link}}" class="btn btn-primary" target="_blank">{{button.text}}</a>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section>
            {{#with city}}
            <h2>{{title}}</h2>

            <div class="row gx-3 gy-4">
                <div class="col-6">
                    <div class="card">
                        <img src="{{cards.[0].picture.src}}" class="card-img-top" alt="{{cards.[0].picture.alt}}">
                        <div class="card-body">
                            <h5 class="card-title">{{cards.[0].title}}</h5>
                            <p class="card-text">{{cards.[0].description}}</p>
                            <a href="{{cards.[0].button.href}}" class="btn btn-primary"
                                target="_blank">{{cards.[0].button.text}}</a>
                        </div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="card">
                        <img src="{{cards.[1].picture.src}}" class="card-img-top" alt="{{cards.[1].picture.alt}}">
                        <div class="card-body">
                            <h5 class="card-title">{{cards.[1].title}}</h5>
                            <p class="card-text">{{cards.[1].description}}</p>
                            <a href="{{cards.[1].button.href}}" class="btn btn-primary"
                                target="_blank">{{cards.[1].button.text}}</a>
                        </div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="card">
                        <img src="{{cards.[2].picture.src}}" class="card-img-top" alt="{{cards.[2].picture.alt}}">
                        <div class="card-body">
                            <h5 class="card-title">{{cards.[2].title}}</h5>
                            <p class="card-text">{{cards.[2].description}}</p>
                            <a href="{{cards.[2].button.href}}" class="btn btn-primary"
                                target="_blank">{{cards.[2].button.text}}</a>
                        </div>
                    </div>
                </div>

                <div class="col-6">
                    <div class="card">
                        <img src="{{cards.[3].picture.src}}" class="card-img-top" alt="{{cards.[3].picture.alt}}">
                        <div class="card-body">
                            <h5 class="card-title">{{cards.[3].title}}</h5>
                            <p class="card-text">{{cards.[3].description}}</p>
                            <a href="{{cards.[3].button.href}}" class="btn btn-primary"
                                target="_blank">{{cards.[3].button.text}}</a>
                        </div>
                    </div>
                </div>
            </div>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section>
            {{#with winner}}
            <h2>{{title}}</h2>

            <table class="table">
                {{#with table}}
                <thead>
                    <tr>
                        {{#each head}}
                        <th>{{this}}</th>
                        {{/each}}
                    </tr>
                </thead>

                <tbody>
                    <tr>
                        <th>{{user.[0].place}}</th>
                        <td>{{user.[0].name}}</td>
                        <td>{{user.[0].surname}}</td>
                        <td>{{user.[0].nickname}}</td>
                    </tr>
                    <tr>
                        <th>{{user.[1].place}}</th>
                        <td>{{user.[1].name}}</td>
                        <td>{{user.[1].surname}}</td>
                        <td>{{user.[1].nickname}}</td>
                    </tr>
                    <tr>
                        <th>{{user.[2].place}}</th>
                        <td>{{user.[2].name}}</td>
                        <td>{{user.[2].surname}}</td>
                        <td>{{user.[2].nickname}}</td>
                    </tr>
                </tbody>
                {{/with}}
            </table>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section class="row g-3">
            {{#with gallery}}
            <h2>{{title}}</h2>
            <div class="col-4">
                <div class="card">
                    <div class="card-header">
                        {{cards.[0].name}}
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item">{{cards.[0].items.[0]}}</li>
                        <li class="list-group-item">{{cards.[0].items.[1]}}</li>
                        <li class="list-group-item">{{cards.[0].items.[2]}}</li>
                    </ul>
                </div>
            </div>

            <div class="col-4">
                <div class="card">
                    <div class="card-header">
                        {{cards.[1].name}}
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item">{{cards.[1].items.[0]}}</li>
                        <li class="list-group-item">{{cards.[1].items.[1]}}</li>
                        <li class="list-group-item">{{cards.[1].items.[2]}}</li>
                    </ul>
                </div>
            </div>

            <div class="col-4">
                <div class="card">
                    <div class="card-header">
                        {{cards.[2].name}}
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item">{{cards.[2].items.[0]}}</li>
                        <li class="list-group-item">{{cards.[2].items.[1]}}</li>
                        <li class="list-group-item">{{cards.[2].items.[2]}}</li>
                    </ul>
                </div>
            </div>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section class="row g-3">
            {{#with relaxion}}
            <div class="display-3">
                {{title}}
            </div>

            <div class="col-6">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{cards.[0].header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{cards.[0].title}}</h5>
                        <p class="card-text">{{cards.[0].description}}</p>
                    </div>
                </div>
            </div>
            <div class="col-6">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{cards.[1].header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{cards.[1].title}}</h5>
                        <p class="card-text">{{cards.[1].description}}</p>
                    </div>
                </div>
            </div>
            <div class="col-6">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{cards.[2].header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{cards.[2].title}}</h5>
                        <p class="card-text">{{cards.[2].description}}</p>
                    </div>
                </div>
            </div>
            <div class="col-6">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{cards.[3].header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{cards.[3].title}}</h5>
                        <p class="card-text">{{cards.[3].description}}</p>
                    </div>
                </div>
            </div>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section class="row gy-4">
            {{#with rest}}
            <div class="col-6">
                <div class="card">
                    <div class="card-header text-center"> {{cards.[0].header}}</div>
                    <div class="card-body text-center">
                        <h5 class="card-title">{{cards.[0].title}}</h5>
                        <p class="card-text">{{cards.[0].description}}</p>
                        <a href="#" class="btn btn-primary">{{cards.[0].button}}</a>
                    </div>
                    <div class="card-footer text-center">{{cards.[0].footer}}</div>
                </div>
            </div>
            <div class="col-6">
                <div class="card">
                    <div class="card-header text-center"> {{cards.[1].header}}</div>
                    <div class="card-body text-center">
                        <h5 class="card-title">{{cards.[1].title}}</h5>
                        <p class="card-text">{{cards.[1].description}}</p>
                        <a href="#" class="btn btn-primary">{{cards.[1].button}}</a>
                    </div>
                    <div class="card-footer text-center">{{cards.[1].footer}}</div>
                </div>
            </div>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section class="row g-4">
            {{#with proposition}}
            <div class="display-2">
                {{title}}
            </div>
            <div class="col-5">
                <div class="card text-bg-warning">
                    <div class="card-header">{{card.[0].header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{card.[0].title}}</h5>
                        <p class="card-text">{{card.[0].description}}</p>
                    </div>
                </div>
            </div>
            <div class="col-5">
                <div class="card text-bg-warning">
                    <div class="card-header">{{card.[1].header}}</div>
                    <div class="card-body">
                        <h5 class="card-title">{{card.[1].title}}</h5>
                        <p class="card-text">{{card.[1].description}}</p>
                    </div>
                </div>
            </div>
            {{/with}}
        </section>
        <br>
        <hr>
        <br>

        <section class="row g-5">
            {{#with footer}}
            <h5>{{title}}</h5>
            <div class="col-4">
                <a href="{{navigation.[0].href}}" class="btn btn-link" target="_blank">
                    {{navigation.[0].text}}
                </a><br>
                <a href="{{navigation.[1].href}}" class="btn btn-link" target="_blank">
                    {{navigation.[1].text}}
                </a><br>
                <a href="{{navigation.[2].href}}" class="btn btn-link" target="_blank">
                    {{navigation.[2].text}}
                </a><br>
            </div>
            <div class="col-4">
                <a href="{{socialMedia.[0].link}}" class="btn btn-outline-dark" target="_blank">
                    {{socialMedia.[0].name}}\
                </a><br>
                <a href="{{socialMedia.[1].link}}" class="btn btn-outline-dark" target="_blank">
                    {{socialMedia.[1].name}}
                </a><br>
                <a href="{{socialMedia.[2].link}}" class="btn btn-outline-dark" target="_blank">
                    {{socialMedia.[2].name}}
                </a><br>
            </div>
            <div class="col-4">
                <a href="{{partners.[0].link}}" class="btn btn-outline-info" target="_blank">
                    {{partners.[0].name}}\
                </a><br>
                <a href="{{partners.[1].link}}" class="btn btn-outline-info" target="_blank">
                    {{partners.[1].name}}
                </a><br>
                <a href="{{partners.[2].link}}" class="btn btn-outline-info" target="_blank">
                    {{partners.[2].name}}
                </a><br>
            </div>
            {{/with}}
        </section>
    </div>
</main>